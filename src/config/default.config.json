{
  "watchFolder": "C:/Users/Demian/Downloads",
  "checkInterval": 1000,
  "rules": [
    {
      "nameContains": "Protocolo",
      "destination": "C:/Users/Demian/Downloads/Facultad"
    },
    {
      "nameContains": "PPS",
      "destination": "C:/Users/Demian/Downloads/Facultad"
    },
    {
      "extensions": [".exe", ".msi"],
      "destination": "C:/Users/Demian/Downloads/Ejecutables"
    },
    {
      "extensions": [".jpg", ".jpeg", ".png", ".gif", ".bmp", ".webp", ".tiff"],
      "destination": "C:/Users/Demian/Downloads/Imagenes"
    },
    {
      "extensions": [".zip", ".rar", ".7z", ".tar", ".gz", ".xz"],
      "destination": "C:/Users/Demian/Downloads/Comprimidos"
    },
    {
      "extensions": [".pdf", ".doc", ".docx", ".txt", ".odt", ".xls", ".xlsx", ".ppt", ".pptx"],
      "destination": "C:/Users/Demian/Downloads/Documentos"
    }
  ]
}
